<form class="{{cssClass}}" autocomplete="off" id="form_persona">
    <header class="sheet-header">
        <div class='container'>
            <div class="row container_dados">
                <div class="col-md-4 col-4">
                    <a href="https://www.tagmar.com.br/wiki/search.aspx" class="tagmarlink"><img class="tagmarLogo" src="systems/tagmar_rpg/templates/sheets/img/logo.png"/></a>  
                </div>
                <div class="col-md-6 col-6">
                    <h1 class="fairyDust" style="text-align: center; padding-top:20px;">{{actor.type}}</h1>
                </div>
                <div class="col-md-2"></div>
            </div>
            <div class="row container_dados">
                <div class="col-md-2 col-4" style="padding-top: 3px;">
                    <img src="{{actor.img}}" data-edit="img" title="{{actor.name}}" height="64" width="64" style="border-radius: 8px;"/>
                </div>
                <div class="col-md-6 col-8">
                    <h1 class="fairyDust"><input name="name" type="text" value="{{actor.name}}" placeholder="Name"/></h1>
                </div>
                <div class="col-md-2 col-4" style="text-align: center;">
                    <label><h1 class="fairyDust" style="padding-top: 15px;">Estágio:</h1></label>
                </div>
                <div class="col-md-2 col-8">
                    <h1 class="fairyDust"><input class="ipEstagio" name="system.estagio" type="number" value="{{data.system.estagio}}" style="text-align: center";/></h1>
                </div>  
            </div>
        </div>
    </header>
    <section class="sheet-primary form_tagmar">
        <div class="container container_dados">
            <div class="row dados">
                <div class="col-md-12">
                    <nav class="prim-tabs tabs" data-group="primary">       <!--Tabs-->
                        <label class="mediaeval" style="border:black double 1px; text-align: center;"><a class="item" data-tab="basico" title="Basico">Básico</a></label>
                        <label class="mediaeval" style="border:black double 1px; text-align: center;"><a class="item" data-tab="habilidades" title="Habilidades">Habilidades</a></label>
                        <label class="mediaeval" style="border:black double 1px; text-align: center;"><a class="item" data-tab="combate" title="Combate">Combate</a></label>
                        <label class="mediaeval" style="border:black double 1px; text-align: center;"><a class="item" data-tab="magia" title="Magia">Magia</a></label>
                        <label class="mediaeval" style="border:black double 1px; text-align: center;"><a class="item" data-tab="pertences" title="Pertences">Pertences</a></label>
                        <label class="mediaeval" style="border:black double 1px; text-align: center;"><a class="item" data-tab="efeitos" title="Efeitos">Efeitos</a></label>
                        <label class="mediaeval" style="border:black double 1px; text-align: center;"><a class="item" data-tab="descricao" title="Biografia">Biografia</a></label>
                        <label class="mediaeval" style="border:black double 1px; text-align: center;"><a class="item" data-tab="favoritos" title="Favoritos">Favoritos</a></label>
                    </nav>
                </div>
            </div>
            <div class="tab basico container" data-group="primary" data-tab="basico"> <!--Tab Básico-->
                <div class="row dados">         <!--Primeira Linha-->
                    <div class="col-md-1 col-4">
                        {{#ifeq actor.raca ""}}
                        <label><h4 class="mediaeval">Raça:</h4></label>
                        {{/ifeq}}
                        {{#ifdf actor.raca ""}}
                        <a class="displayRaca"><h4 class="mediaeval">Raça:</h4></a>
                        {{/ifdf}}
                    </div>
                    <div class="col-md-3 col-8">
                        <input name="system.raca" type="text" list="racas" value="{{data.system.raca}}" height="30" title="Escolha uma raça ou crie a sua." readonly/>
                    </div>
                    <div class="col-md-2 col-4">
                        {{#ifeq actor.profissao ""}}
                        <label><h4 class="mediaeval">Profissão:</h4></label>
                        {{/ifeq}}
                        {{#ifdf actor.profissao ""}}
                        <a class="displayProf"><h4 class="mediaeval">Profissão:</h4></a>
                        {{/ifdf}}
                    </div>
                    <div class="col-md-3 col-8">
                        <input class="profissao" name="system.profissao" type="text" value="{{data.system.profissao}}" height="30" title="Uma profissão diz muito sobre o herói." readonly/>
                    </div>
                    <div class="col-md-1 col-4">
                        <label><h4 class="mediaeval">Deus:</h4></label>
                    </div>
                    <div class="col-md-2 col-8">
                        <input name="system.deus" type="text" list="deuses" value="{{data.system.deus}}" height="30" title="Tem uns que acreditam, outros não."/>
                        <datalist id="deuses">
                            <option value="Blator"></option>
                            <option value="Cambu"></option>
                            <option value="Crezir"></option>
                            <option value="Crisagom"></option>
                            <option value="Cruine"></option>
                            <option value="Ganis"></option>
                            <option value="Lena"></option>
                            <option value="Liris"></option>
                            <option value="Maira"></option>
                            <option value="Palier"></option>
                            <option value="Parom"></option>
                            <option value="Plandis"></option>
                            <option value="Quiris"></option>
                            <option value="Selimon"></option>
                            <option value="Sevides"></option>
                        </datalist>
                    </div>
                </div>
                <div class="row dados">         <!--Segunda Linha-->
                    <div class="col-md-2 col-4">
                        <label><h4 class="mediaeval">Classe Social:</h4></label>
                    </div>
                    <div class="col-md-2 col-8">
                        <input name="system.classe_social" type="text" value="{{data.system.classe_social}}" height="30" title="Suas atitutes demonstram sua falta de classe."/>
                    </div>
                    <div class="col-md-2 col-4">
                        <label><h4 class="mediaeval">Especialização:</h4></label>
                    </div>
                    <div class="col-md-4 col-8">
                        <select name="system.especializacao" type="text" data-dtype="String" value="{{data.system.especializacao}}" height="30" title="Caia na real, você não tem nada de especial. Se você escolheu uma profissão, pode escolher algo aqui tambem."/>
                            {{#selectOptions actor.especializacoes selected=data.system.especializacao valueAttr="key"}}
                            {{/selectOptions}}
                        </select>
                    </div>
                    <div class="col-md-1 col-4">
                        <label><h4 class="mediaeval">Idade:</h4></label>
                    </div>
                    <div class="col-md-1 col-8">
                        <input name="system.idade" type="number" value="{{data.system.idade}}" height="30" title="Só isso? Parece bem mais velho."/>
                    </div>
                </div>
                <div class="row dados">         <!--Terceira Linha-->
                    <div class="col-md-2 col-4">
                        <h4 class="mediaeval">Altura(m):</h4>
                    </div>
                    <div class="col-md-2 col-8">
                        <input name="system.altura" type="text" value="{{data.system.altura}}" height="30" title="Altura do personagem, não a sua."/>
                    </div>
                    <div class="col-md-1 col-4">
                        <h4 class="mediaeval">Peso(Kg):</h4>
                    </div>
                    <div class="col-md-2 col-8">
                        <input style="margin-left: 25px;"  name="system.peso" type="number" value="{{data.system.peso}}" height="30" title="Não adianta encolher a barriga."/>
                    </div>
                    <div class="col-md-2 col-4" style="text-align: right;">
                        <h4 class="mediaeval">Carga(Kg):</h4>
                    </div>
                    <div class="col-md-2 col-6">
                        <input title="Carga Máxima: 20Kg p/ ponto de FOR" name="system.carga.value" type="number" value="{{toFixed data.system.carga.value 2}}" height="30" readonly/>
                    </div>
                    <div class="col-md-1 col-2">
                        {{#if data.system.carga.sobrecarga}}
                            <i class="fas fa-weight-hanging sobrepeso" style="text-align: center;"><h4 class="mediaeval">+{{toFixed data.system.carga.valor_s 2}}Kg</h4></i>
                        {{else}}
                            <i class="fas fa-weight-hanging sobrepeso hiddenT"></i>
                        {{/if}}
                    </div>
                </div>
                <div class="row dados">  <!-- Table Atributos -->       <!--Quarta Linha-->
                    <div class="col-md-12">
                        <table>
                            <tr style="text-align: left;">
                                <th width="150px" class="mediaeval">Pontos: {{toFixed data.system.carac_final.INT 1}}</th>
                                <th class="mediaeval"><a class="rolarAtt" data-item-id="INT" title="Clique para rolar Teste.">INT</a></th>
                                <th class="mediaeval"><a class="rolarAtt" data-item-id="AUR" title="Clique para rolar Teste.">AUR</a></th>
                                <th class="mediaeval"><a class="rolarAtt" data-item-id="CAR" title="Clique para rolar Teste.">CAR</a></th>
                                <th class="mediaeval"><a class="rolarAtt" data-item-id="FOR" title="Clique para rolar Teste.">FOR</a></th>
                                <th class="mediaeval"><a class="rolarAtt" data-item-id="FIS" title="Clique para rolar Teste.">FIS</a></th>
                                <th class="mediaeval"><a class="rolarAtt" data-item-id="AGI" title="Já entendeu né?">AGI</a></th>
                                <th class="mediaeval"><a class="rolarAtt" data-item-id="PER" title="Clique para rolar Teste.">PER</a></th>
                            </tr>
                            <tr>
                                <td class="mediaeval">Modificador Racial</td>
                                <td><input title="Ajuste de INT da raça." class="mod_racialINT" name="system.mod_racial.INT" type="number" value="{{data.system.mod_racial.INT}}" readonly/></td>
                                <td><input title="Ajuste de AUR da raça." class="mod_racialAUR" name="system.mod_racial.AUR" type="number" value="{{data.system.mod_racial.AUR}}" readonly/></td>
                                <td><input title="Ajuste de CAR da raça." class="mod_racialCAR" name="system.mod_racial.CAR" type="number" value="{{data.system.mod_racial.CAR}}" readonly/></td>
                                <td><input title="Ajuste de FOR da raça." class="mod_racialFOR" name="system.mod_racial.FOR" type="number" value="{{data.system.mod_racial.FOR}}" readonly/></td>
                                <td><input title="Ajuste de FIS da raça." class="mod_racialFIS" name="system.mod_racial.FIS" type="number" value="{{data.system.mod_racial.FIS}}" readonly/></td>
                                <td><input title="Ajuste de AGI da raça." class="mod_racialAGI" name="system.mod_racial.AGI" type="number" value="{{data.system.mod_racial.AGI}}" readonly/></td>
                                <td><input title="Ajuste de PER da raça." class="mod_racialPER" name="system.mod_racial.PER" type="number" value="{{data.system.mod_racial.PER}}" readonly/></td>
                            </tr>
                            <tr>
                                <td class="mediaeval">Pontos Gastos</td>
                                <td><input title="Valor sorteado." class="carac_sortINT" name="system.carac_sort.INT" type="number" value="{{data.system.carac_sort.INT}}" readonly/></td>
                                <td><input title="Valor sorteado." class="carac_sortAUR" name="system.carac_sort.AUR" type="number" value="{{data.system.carac_sort.AUR}}" readonly/></td>
                                <td><input title="Valor sorteado." class="carac_sortCAR" name="system.carac_sort.CAR" type="number" value="{{data.system.carac_sort.CAR}}" readonly/></td>
                                <td><input title="Valor sorteado." class="carac_sortFOR" name="system.carac_sort.FOR" type="number" value="{{data.system.carac_sort.FOR}}" readonly/></td>
                                <td><input title="Valor sorteado." class="carac_sortFIS" name="system.carac_sort.FIS" type="number" value="{{data.system.carac_sort.FIS}}" readonly/></td>
                                <td><input title="Valor sorteado." class="carac_sortAGI" name="system.carac_sort.AGI" type="number" value="{{data.system.carac_sort.AGI}}" readonly/></td>
                                <td><input title="Valor sorteado." class="carac_sortPER" name="system.carac_sort.PER" type="number" value="{{data.system.carac_sort.PER}}" readonly/></td>
                            </tr>
                            <tr>
                                <td class="mediaeval">Ajuste</td>
                                <td><input title="Atributo final do personagem." name="system.atributos.INT" type="number" value="{{data.system.atributos.INT}}" /></td>
                                <td><input title="Atributo final do personagem." class="ipAtrAUR" name="system.atributos.AUR" type="number" value="{{data.system.atributos.AUR}}" /></td>
                                <td><input title="Atributo final do personagem." name="system.atributos.CAR" type="number" value="{{data.system.atributos.CAR}}" /></td>
                                <td><input title="Atributo final do personagem." name="system.atributos.FOR" type="number" value="{{data.system.atributos.FOR}}" /></td>
                                <td><input title="Atributo final do personagem." class="ipAtrFIS" name="system.atributos.FIS" type="number" value="{{data.system.atributos.FIS}}" /></td>
                                <td><input title="Atributo final do personagem." name="system.atributos.AGI" type="number" value="{{data.system.atributos.AGI}}" /></td>
                                <td><input title="Atributo final do personagem." name="system.atributos.PER" type="number" value="{{data.system.atributos.PER}}" /></td>
                            </tr>
                            <tr>
                                <td class="mediaeval">Valor Para Teste</td>
                                <td><input title="Simplesmente 4 vezes o valor do atributo." name="system.valor_teste.INT" type="number" value="{{data.system.valor_teste.INT}}" readonly/></td>
                                <td><input title="Simplesmente 4 vezes o valor do atributo." name="system.valor_teste.AUR" type="number" value="{{data.system.valor_teste.AUR}}" readonly/></td>
                                <td><input title="Simplesmente 4 vezes o valor do atributo." name="system.valor_teste.CAR" type="number" value="{{data.system.valor_teste.CAR}}" readonly/></td>
                                <td><input title="Simplesmente 4 vezes o valor do atributo." name="system.valor_teste.FOR" type="number" value="{{data.system.valor_teste.FOR}}" readonly/></td>
                                <td><input title="Simplesmente 4 vezes o valor do atributo." name="system.valor_teste.FIS" type="number" value="{{data.system.valor_teste.FIS}}" readonly/></td>
                                <td><input title="Simplesmente 4 vezes o valor do atributo." name="system.valor_teste.AGI" type="number" value="{{data.system.valor_teste.AGI}}" readonly/></td>
                                <td><input title="Simplesmente 4 vezes o valor do atributo." name="system.valor_teste.PER" type="number" value="{{data.system.valor_teste.PER}}" readonly/></td>
                            </tr>
                        </table>
                    </div>
                </div>
                <div class="row dados">     <!--Quinta Linha-->
                    <div class="col-md-12">
                        <table>
                            <caption class="mediaeval">Pontos para Grupo de Armas: {{data.system.pontos_comb}}</caption>
                            <tr style="text-align: center;">
                                <th class="mediaeval">CD</th>
                                <th class="mediaeval">CI</th>
                                <th class="mediaeval">CL</th>
                                <th class="mediaeval">CLD</th>
                                <th class="mediaeval">EL</th>
                                <th class="mediaeval">CmE</th>
                                <th class="mediaeval">CmM</th>
                                <th class="mediaeval">EM</th>
                                <th class="mediaeval">PmA</th>
                                <th class="mediaeval">PmL</th>
                                <th class="mediaeval">CpE</th>
                                <th class="mediaeval">CpM</th>
                                <th class="mediaeval">EP</th>
                                <th class="mediaeval">PP</th>
                                <th class="mediaeval">PpA</th>
                                <th class="mediaeval">PpB</th>
                            </tr>
                            {{#if actor.profissao.system.grupo_aprim}}
                                {{#ifge data.system.estagio 5}}
                                    <tr>
                                        <td style="text-align: center;"><input type="checkbox" name="grupo_aprim" value="CD" {{checked actor.cd_aprim}} title="Grupo de Armas Aprimorado"/></td>
                                        <td style="text-align: center;"><input type="checkbox" name="grupo_aprim" value="CL" {{checked actor.cl_aprim}} title="Grupo de Armas Aprimorado"/></td>
                                        <td style="text-align: center;"><input type="checkbox" name="grupo_aprim" value="CI" {{checked actor.ci_aprim}} title="Grupo de Armas Aprimorado"/></td>
                                        <td style="text-align: center;"><input type="checkbox" name="grupo_aprim" value="CLD" {{checked actor.cld_aprim}} title="Grupo de Armas Aprimorado"/></td>
                                        <td style="text-align: center;"><input type="checkbox" name="grupo_aprim" value="EL" {{checked actor.el_aprim}} title="Grupo de Armas Aprimorado"/></td>
                                        <td style="text-align: center;"><input type="checkbox" name="grupo_aprim" value="CmE" {{checked actor.cme_aprim}} title="Grupo de Armas Aprimorado"/></td>
                                        <td style="text-align: center;"><input type="checkbox" name="grupo_aprim" value="CmM" {{checked actor.cmm_aprim}} title="Grupo de Armas Aprimorado"/></td>
                                        <td style="text-align: center;"><input type="checkbox" name="grupo_aprim" value="EM" {{checked actor.em_aprim}} title="Grupo de Armas Aprimorado"/></td>
                                        <td style="text-align: center;"><input type="checkbox" name="grupo_aprim" value="PmA" {{checked actor.pma_aprim}} title="Grupo de Armas Aprimorado"/></td>
                                        <td style="text-align: center;"><input type="checkbox" name="grupo_aprim" value="PmL" {{checked actor.pml_aprim}} title="Grupo de Armas Aprimorado"/></td>
                                        <td style="text-align: center;"><input type="checkbox" name="grupo_aprim" value="CpE" {{checked actor.cpe_aprim}} title="Grupo de Armas Aprimorado"/></td>
                                        <td style="text-align: center;"><input type="checkbox" name="grupo_aprim" value="CpM" {{checked actor.cpm_aprim}} title="Grupo de Armas Aprimorado"/></td>
                                        <td style="text-align: center;"><input type="checkbox" name="grupo_aprim" value="EP" {{checked actor.ep_aprim}} title="Grupo de Armas Aprimorado"/></td>
                                        <td style="text-align: center;"><input type="checkbox" name="grupo_aprim" value="PP" {{checked actor.pp_aprim}} title="Grupo de Armas Aprimorado"/></td>
                                        <td style="text-align: center;"><input type="checkbox" name="grupo_aprim" value="PpA" {{checked actor.ppa_aprim}} title="Grupo de Armas Aprimorado"/></td>
                                        <td style="text-align: center;"><input type="checkbox" name="grupo_aprim" value="PpB" {{checked actor.ppb_aprim}} title="Grupo de Armas Aprimorado"/></td>
                                    </tr>
                                {{/ifge}}
                            {{/if}}
                            <tr style="text-align: center;">
                                <td><a class="addGrupoArmas" data-item-id="CD"><i class="fas fa-plus-circle"></i></a></td>
                                <td><a class="addGrupoArmas" data-item-id="CI"><i class="fas fa-plus-circle"></i></a></td>
                                <td><a class="addGrupoArmas" data-item-id="CL"><i class="fas fa-plus-circle"></i></a></td>
                                <td><a class="addGrupoArmas" data-item-id="CLD"><i class="fas fa-plus-circle"></i></a></td>
                                <td><a class="addGrupoArmas" data-item-id="EL"><i class="fas fa-plus-circle"></i></a></td>
                                <td><a class="addGrupoArmas" data-item-id="CmE"><i class="fas fa-plus-circle"></i></a></td>
                                <td><a class="addGrupoArmas" data-item-id="CmM"><i class="fas fa-plus-circle"></i></a></td>
                                <td><a class="addGrupoArmas" data-item-id="EM"><i class="fas fa-plus-circle"></i></a></td>
                                <td><a class="addGrupoArmas" data-item-id="PmA"><i class="fas fa-plus-circle"></i></a></td>
                                <td><a class="addGrupoArmas" data-item-id="PmL"><i class="fas fa-plus-circle"></i></a></td>
                                <td><a class="addGrupoArmas" data-item-id="CpE"><i class="fas fa-plus-circle"></i></a></td>
                                <td><a class="addGrupoArmas" data-item-id="CpM"><i class="fas fa-plus-circle"></i></a></td>
                                <td><a class="addGrupoArmas" data-item-id="EP"><i class="fas fa-plus-circle"></i></a></td>
                                <td><a class="addGrupoArmas" data-item-id="PP"><i class="fas fa-plus-circle"></i></a></td>
                                <td><a class="addGrupoArmas" data-item-id="PpA"><i class="fas fa-plus-circle"></i></a></td>
                                <td><a class="addGrupoArmas" data-item-id="PpB"><i class="fas fa-plus-circle"></i></a></td>
                            </tr>
                            <tr style="text-align: center;">
                                <td><input title="Combate Desarmado" name="system.grupos.CD" type="number" value="{{data.system.grupos.CD}}" readonly/></td>
                                <td><input title="Combate de Imobilização" name="system.grupos.CI" type="number" value="{{data.system.grupos.CI}}" readonly/></td>
                                <td><input title="Corte Leve" name="system.grupos.CL" type="number" value="{{data.system.grupos.CL}}" readonly/></td>
                                <td><input title="Combate Leve à Distância" name="system.grupos.CLD" type="number" value="{{data.system.grupos.CLD}}" readonly/></td>
                                <td><input title="Esmagamento Leve" name="system.grupos.EL" type="number" value="{{data.system.grupos.EL}}"readonly /></td>
                                <td><input title="Corte Médio Com Espadas" name="system.grupos.CmE" type="number" value="{{data.system.grupos.CmE}}" readonly/></td>
                                <td><input title="Corte Médio Com Machados" name="system.grupos.CmM" type="number" value="{{data.system.grupos.CmM}}" readonly/></td>
                                <td><input title="Esmagamento Médio" name="system.grupos.EM" type="number" value="{{data.system.grupos.EM}}" readonly/></td>
                                <td><input title="Penetração Média Com Arcos" name="system.grupos.PmA" type="number" value="{{data.system.grupos.PmA}}" readonly/></td>
                                <td><input title="Penetração Média Com Lanças" name="system.grupos.PmL" type="number" value="{{data.system.grupos.PmL}}" readonly/></td>
                                <td><input title="Corte Pesado Com Espadas" name="system.grupos.CpE" type="number" value="{{data.system.grupos.CpE}}" readonly/></td>
                                <td><input title="Corte Pesado Com Machados" name="system.grupos.CpM" type="number" value="{{data.system.grupos.CpM}}" readonly/></td>
                                <td><input title="Esmagamento Pesado" name="system.grupos.EP" type="number" value="{{data.system.grupos.EP}}" readonly/></td>
                                <td><input title="Penetração Pesada" name="system.grupos.PP" type="number" value="{{data.system.grupos.PP}}" readonly/></td>
                                <td><input title="Pentração Pesado Com Arcos" name="system.grupos.PpA" type="number" value="{{data.system.grupos.PpA}}" readonly/></td>
                                <td><input title="Pentração Pesado Com Bestas" name="system.grupos.PpB" type="number" value="{{data.system.grupos.PpB}}" readonly/></td>
                            </tr>
                            <tr style="text-align: center;">
                                <td><a class="subGrupoArmas" data-item-id="CD"><i class="fas fa-minus-circle"></i></a></td>
                                <td><a class="subGrupoArmas" data-item-id="CI"><i class="fas fa-minus-circle"></i></a></td>
                                <td><a class="subGrupoArmas" data-item-id="CL"><i class="fas fa-minus-circle"></i></a></td>
                                <td><a class="subGrupoArmas" data-item-id="CLD"><i class="fas fa-minus-circle"></i></a></td>
                                <td><a class="subGrupoArmas" data-item-id="EL"><i class="fas fa-minus-circle"></i></a></td>
                                <td><a class="subGrupoArmas" data-item-id="CmE"><i class="fas fa-minus-circle"></i></a></td>
